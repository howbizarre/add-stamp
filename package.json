{
  "name": "add-stamp",
  "version": "1.0.5",
  "description": "A Nuxt 4 application for applying watermarks to images using WebAssembly (WASM) for high-performance image processing",
  "keywords": [
    "nuxt4",
    "vue3",
    "typescript",
    "tailwindcss",
    "webassembly",
    "wasm",
    "rust",
    "image-processing",
    "watermark",
    "stamp",
    "batch-processing",
    "cloudflare-workers",
    "browser",
    "images",
    "image-processing",
    "batch-processing",
    "wrangler"
  ],
  "homepage": "https://github.com/howbizarre/add-stamp#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/howbizarre/add-stamp.git"
  },
  "bugs": {
    "url": "https://github.com/howbizarre/add-stamp/issues"
  },
  "author": {
    "name": "howbizarre",
    "url": "https://github.com/howbizarre"
  },
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:wasm && nuxt build",
    "build:wasm": "cd wasm && wasm-pack build --target web --out-dir pkg --out-name image_stamper && cd .. && npm run copy:wasm",
    "copy:wasm": "node -e \"const fs=require('fs'),path=require('path');const src='wasm/pkg',dest='public/wasm';if(!fs.existsSync(dest))fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src).forEach(f=>fs.copyFileSync(path.join(src,f),path.join(dest,f)));\"",
    "dev": "nuxt dev --port 5654",
    "dev:wasm": "npm run build:wasm && npm run dev",
    "generate": "nuxt generate",
    "preview": "npm run build && wrangler dev",
    "postinstall": "nuxt prepare",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "clean:wasm": "node -e \"const fs=require('fs'),path=require('path');const rmrf=p=>{if(fs.existsSync(p)){if(fs.lstatSync(p).isDirectory()){fs.readdirSync(p).forEach(f=>rmrf(path.join(p,f)));fs.rmdirSync(p)}else{fs.unlinkSync(p)}}};rmrf('wasm/pkg');['js','wasm','d.ts'].forEach(ext=>fs.readdirSync('public/wasm').filter(f=>f.endsWith('.'+ext)).forEach(f=>fs.unlinkSync(path.join('public/wasm',f))));\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "nuxt": "^4.0.0",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.12.3",
    "wrangler": "^4.25.0"
  }
}